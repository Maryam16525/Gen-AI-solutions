# config for streamlit


#[theme]
#base="light"
#primaryColor="#ea7200"
#secondaryBackgroundColor="#f9f9f9"
#textColor="#000000"


[server]
port = 8000
maxUploadSize = 3000
maxMessageSize = 3000
enableXsrfProtection = false
enableCORS = true

[client]
showErrorDetails = "none"


[browser]
gatherUsageStats = false
#serverAddress = "localhost"
